<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CreateAccountController">
    <children>
        <Label layoutX="131.0" layoutY="43.0" text="Create Account" />
        <Label layoutX="49.0" layoutY="97.0" text="Username" />
        <Label layoutX="49.0" layoutY="133.0" text="Password" />
        <Label layoutX="49.0" layoutY="169.0" text="Confirm Password" />
        <TextField fx:id="usernameField" layoutX="150.0" layoutY="93.0" prefWidth="150.0" />
        <PasswordField fx:id="passwordField" layoutX="150.0" layoutY="129.0" prefWidth="150.0" />
        <PasswordField fx:id="confirmPasswordField" layoutX="150.0" layoutY="165.0" prefWidth="150.0" />
        <Button fx:id="submitButton" layoutX="134.0" layoutY="209.0" mnemonicParsing="false" onAction="#handleSubmitButtonAction" text="Submit" />
        <Button fx:id="backButton" layoutX="209.0" layoutY="209.0" mnemonicParsing="false" onAction="#handleBackButtonAction" text="Back" />
        <Label fx:id="errorMessageLabel" layoutX="49.0" layoutY="240.0" prefWidth="252.0" textFill="#ff0000" />
    </children>
</AnchorPane>
